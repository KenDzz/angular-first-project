<?xml version="1.0" encoding="UTF-8" ?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file source-language="en" datatype="plaintext" original="ng2.template">
    <body>
      <!-- Auth Login Messages -->
      <trans-unit id="auth.login.welcome" datatype="html">
        <source>Welcome Back</source>
        <note priority="1" from="description">Login page welcome title</note>
      </trans-unit>
      <trans-unit id="auth.login.subtitle" datatype="html">
        <source>Sign in to your account to continue</source>
        <note priority="1" from="description">Login page subtitle</note>
      </trans-unit>
      <trans-unit id="auth.email.label" datatype="html">
        <source>Email Address</source>
        <note priority="1" from="description">Email field label</note>
      </trans-unit>
      <trans-unit id="auth.email.placeholder" datatype="html">
        <source>Enter your email</source>
        <note priority="1" from="description">Email field placeholder</note>
      </trans-unit>
      <trans-unit id="auth.password.label" datatype="html">
        <source>Password</source>
        <note priority="1" from="description">Password field label</note>
      </trans-unit>
      <trans-unit id="auth.password.placeholder" datatype="html">
        <source>Enter your password</source>
        <note priority="1" from="description">Password field placeholder</note>
      </trans-unit>
      <trans-unit id="auth.login.signing-in" datatype="html">
        <source>Signing in...</source>
        <note priority="1" from="description">Login button loading text</note>
      </trans-unit>
      <trans-unit id="auth.login.sign-in" datatype="html">
        <source>Sign In</source>
        <note priority="1" from="description">Login button text</note>
      </trans-unit>
      <trans-unit id="auth.login.no-account" datatype="html">
        <source>Don't have an account?</source>
        <note priority="1" from="description">Link to register page text</note>
      </trans-unit>
      <trans-unit id="auth.login.create-account" datatype="html">
        <source>Create one here</source>
        <note priority="1" from="description">Link to register page</note>
      </trans-unit>

      <!-- Auth Register Messages -->
      <trans-unit id="auth.register.title" datatype="html">
        <source>Create Account</source>
        <note priority="1" from="description">Register page title</note>
      </trans-unit>
      <trans-unit id="auth.register.subtitle" datatype="html">
        <source>Sign up to get started with your account</source>
        <note priority="1" from="description">Register page subtitle</note>
      </trans-unit>

      <!-- Dashboard Messages -->
      <trans-unit id="dashboard.title" datatype="html">
        <source>Dashboard</source>
        <note priority="1" from="description">Dashboard page title</note>
      </trans-unit>
      <trans-unit id="dashboard.welcome" datatype="html">
        <source>Welcome back, {firstName}!</source>
        <note priority="1" from="description">Dashboard welcome message</note>
      </trans-unit>
      <trans-unit id="dashboard.sign-out" datatype="html">
        <source>Sign Out</source>
        <note priority="1" from="description">Sign out button text</note>
      </trans-unit>
    </body>
  </file>
</xliff>
